## Basic settings.
set hidden true
set ignorecase true
set icons true

## Custom Functions.
cmd mkdir ${{
  printf "Directory name: "
  read answer
  mkdir $answer
}}

cmd mkfile ${{
  printf "File name: "
  read answer
  touch $answer
}}

## Mappings.
# Bottom item like in Helix.
map ge bottom
# Create things.
map mf mkfile
map md mkdir
